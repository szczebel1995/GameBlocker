    "electron-is-dev": "^1.2.0",
    "electron-reload": "^1.5.0",
    "electron-store": "^5.1.1",
    "emotion-theming": "^10.0.27",
    "lodash": "^4.17.15",
    "mobx": "^5.15.4",
    "mobx-react": "^6.2.2",
    "mobx-state-tree": "^3.16.0",
    "normalize-path": "^3.0.0",
    "ps-list": "^7.0.0",
