{
  "name": "tsbase",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "CI=true jest",
    "test:prod": "testApi=prod npm run test",
    "test:dev": "testApi=dev npm run test",
    "test-watch": "NODE_ENV=dev jest --watch",
    "test-watch:prod": "testApi=prod npm run test-watch",
    "test-watch:dev": "testApi=dev npm run test-watch",
    "watch:serve": "ts-node src/index.ts ",
    "dev-watch": "SET NODE_ENV=dev nodemon -e ts -w ./ -x npm run watch:serve",
    "dev": "SET NODE_ENV=dev npm run watch:serve",
    "prod": "SET NODE_ENV=prod node ./build/index.js",
    "build": "cd src && tsc"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.150",
    "@types/node": "^10.3.5",
    "jest": "^23.6.0",
    "nodemon": "^1.19.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/tests/.*\\.(ts|tsx|js)$"
  },
  "dependencies": {
    "glob": "^7.1.6",
    "lodash": "^4.17.15",
    "ls": "^0.2.1",
    "mobx": "^5.15.4",
    "mobx-state-tree": "^3.16.0",
    "regedit": "^3.0.3"
  }
}
